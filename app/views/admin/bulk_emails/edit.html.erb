<%- return_to = params[:return_to] %>
<h1 class="use16columns inner_left">New Bulk Email Template</h1>

<%= error_messages_for :bulk_email %>

<% form_for :bulk_email, :url => admin_bulk_email_path(@bulk_email), :html => {:method => :put} do |f| -%>
<%= render :partial => 'form' , :locals => { :f => f }%>
<p><%= submit_tag 'Update' %> <%= cancel_tag (return_to=="show" ? admin_bulk_email_path(@bulk_email) : "/admin/bulk_emails") %></p>
<% end -%>
