<style>
 * { font-size: 15px; }
</style>

<h2> Subject story submit timeseries </h2>

<div id="legend" style="float:left;"></div>
<div id="distribution" style="float:left;width:1000px;height:500px"></div>

<script type="text/javascript" src="/js/jquery/jquery.flot.pack.js"> </script>
<script type="text/javascript">
  var opts = { legend : { container: $("#legend") }, xaxis : { mode : "time" } }
  var plot_keys = <%=@plot_keys.to_json%>;
<%- @flot_data_array.each_with_index do |da,i| -%>
  var d_<%=i%> = <%=da.to_json%>;
<%- end -%>
  var data = [];
  for (i = 0; i < <%=@flot_data_array.length%>; i++)
    data[i] = { data: eval("d_" + i), label: "" + plot_keys[i], lines: { show: true, fill: false } }
  $.plot($("#distribution"), data, opts)
</script>
</div>
