<h1 class="use16columns inner_left">New Invitation</h1>

<%= error_messages_for :invitation %>

<div id="invite_form" class="use10columns inner_left">
  <% form_for :invitation, :url => admin_partner_invitations_path(@partner) do |f| -%>
  <%= render :partial => 'form' , :locals => { :f => f }%>
  <p><%= submit_tag 'Create' %> <%= cancel_tag "/admin/partners/#{@partner.to_param}" %></p>
  <% end -%>
</div>
